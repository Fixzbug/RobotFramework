def getData(String key) {
    def dataMap = [
        'Automate': [
            'tag': 'TC001',
            'resultpath': 'TC_login',
            'robotpath' : 'Testcase/SIT/TC_login.robot'
        ]
    ]
    return dataMap[key] ?: 'Key not found'
}

// Return the function for external use
return this